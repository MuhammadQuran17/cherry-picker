{
    "name": "mu/cherry-picker",
    "description": "Automated Git Cherry Picker with Jira, Gitlab integrations for team work",
    "keywords": [
        "mu",
        "cherry-picker"
    ],
    "homepage": "https://github.com/MuhammadQuran17/cherry-picker",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Muhammad Umar",
            "email": "muhammadumarsotvoldiev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/process": "^5.0|^6.0|^7.0|^8.0",
        "laravel/prompts": "^0.1",
        "vlucas/phpdotenv": "^5.0",
        "php-di/php-di": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Mu\\CherryPicker\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "bin": [
        "bin/cherry-picker"
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^4.3"
    }
}
